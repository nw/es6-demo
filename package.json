{
  "name": "simple-webpack",
  "version": "0.0.1",
  "description": "a simple example showing off micro + minimum babel settings",
  "dependencies": {
    "micro-core": "0.3.0"
  },
  "scripts": {
    "start": "webpack --watch",
    "test": "npm run lint",
    "lint": "eslint ./lib",
    "lint-table": "eslint --format table ./lib",
    "lint-html": "eslint --format html ./lib > eslint-result.html; open eslint-result.html;"
  },
  "devDependencies": {
    "babel-core": "~6.10.4",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.9.0",
    "eslint": "~3.0.1",
    "eslint-config-standard": "~5.3.5",
    "eslint-plugin-promise": "~1.3.2",
    "eslint-plugin-standard": "~1.3.3",
    "webpack": "~1.13.1"
  }
}
